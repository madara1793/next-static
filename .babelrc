{
  "presets": [
    "es2015",    
    "next/babel",
    "flow"
  ],
  "plugins": [
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
    "transform-flow-strip-types",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-runtime"
  ]
}
